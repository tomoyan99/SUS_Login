{
  "name": "sus_login",
  "version": "6.0.0",
  "description": "",
  "main": "dist/ts_compiled/src/main/index.js",
  "keywords": [],
  "scripts": {
    "all_build:ins": "run-s ts_compile nexe:build electron:ins",
    "all_build:dir": "run-s ts_compile nexe:build electron:dir",
    "postinstall": "electron-builder install-app-deps --platform win32 --arch x64",
    "package:update": "ncu -u && npm i",
    "ts_compile": "tsc && npm run cp_nonTSfile",
    "cp_nonTSfile": "cp -r src/main/render dist/ts_compiled/src/main && cp -r src/assets dist/ts_compiled/src",
    "electron:start": "tsc && electron .",
    "electron:rebuild": "electron-rebuild -p -m -s",
    "electron:dir": "electron-builder --win --x64 --dir --config electon_builder_congfig.json",
    "electron:ins": "electron-builder --win --x64 --config electon_builder_congfig.json",
    "electron:build_start": "run-s nexe:build electron:start",
    "electron:dir_start": "cd dist/app_build/win-unpacked/ && SUS_Login.exe",
    "webpack": "webpack",
    "nexe:pkg": "ts-node nexe_complie.ts",
    "nexe:build": "run-s webpack nexe:pkg",
    "nexe:start": "npm run nexe:build && powershell dist/app_exe/main.exe",
    "browserTest": "tsc && ts-node ./SUS_Login_core/test/browsertest.ts"
  },
  "author": "Tomoya Sano",
  "license": "ISC",
  "dependencies": {
    "@inquirer/prompts": "^7.2.0",
    "@liveconfig/xterm-webfont": "^2.1.0",
    "@types/blessed": "^0.1.25",
    "@types/electron": "^1.6.12",
    "@types/fontfaceobserver": "^2.1.3",
    "@types/webpack-node-externals": "^3.0.4",
    "@xterm/addon-canvas": "^0.7.0",
    "@xterm/addon-webgl": "^0.18.0",
    "blessed": "^0.1.81",
    "blessed-contrib": "^4.11.0",
    "cli-spinners": "^3.2.0",
    "enquirer": "^2.4.1",
    "eventemitter2": "^6.4.9",
    "node-pty": "^1.0.0",
    "ora": "^8.1.1",
    "puppeteer-core": "^23.10.1",
    "puppeteer-in-electron": "^3.0.5",
    "rcedit": "^4.0.1",
    "readline-promises": "^1.3.1",
    "xterm": "^5.3.0",
    "xterm-addon-canvas": "^0.5.0",
    "xterm-addon-fit": "^0.8.0",
    "xterm-addon-web-links": "^0.9.0",
    "xterm-addon-webgl": "^0.16.0"
  },
  "devDependencies": {
    "bcrypt": "^5.1.1",
    "electron": "31",
    "electron-builder": "^25.1.8",
    "nexe": "^4.0.0-rc.6",
    "node-abi": "^3.71.0",
    "npm-check-updates": "^17.1.11",
    "npm-run-all": "^4.1.5",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2",
    "webpack": "^5.97.1",
    "webpack-cli": "^5.1.4",
    "webpack-node-externals": "^3.0.0",
    "webpack-obfuscator": "^3.5.1"
  },
  "homepage": "./",
  "build": {}
}
