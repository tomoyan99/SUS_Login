{
  "productName": "SUS_Login",
  "appId": "com.electron.SUS_Login",
  "directories": {
    "output": "dist/app_build/"
  },
  "compression": "maximum",
  "files": [
    "dist/ts_compiled/src/**/*.js",
    "src/assets/*",
    "package.json"
  ],
  "extraResources": [
    "dist/app_exe/main.exe"
  ],
  "win": {
    "icon": "src/assets/icon.ico",
    "target": "nsis"
  },
  "nsis": {
    "oneClick": false,
    "allowToChangeInstallationDirectory": true,
    "deleteAppDataOnUninstall": true
  }
}

