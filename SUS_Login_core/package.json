{
  "name": "sus_login",
  "version": "6.0.0",
  "description": "",
  "main": "src/boot/index.cjs",
  "keywords": [],
  "scripts": {
    "node:start": "node SUS_Login_core/terminal_processes/main/main.js",
    "all_build": "run-s nexe:build electron:ins",
    "test": "echo %npm_package_version%",
    "postinstall": "electron-builder install-app-deps --platform win32 --arch x64",
    "package:update": "ncu -u && npm i",
    "electron:start": "electron src/boot/index.cjs",
    "electron:rebuild": "electron-rebuild -m",
    "electron:dir": "electron-builder --win --x64 --dir",
    "electron:ins": "electron-builder --win --x64",
    "webpack": "webpack",
    "nexe:pkg": "node nexe_complie.cjs",
    "nexe:build": "run-s webpack nexe:pkg",
    "exe:start": "powershell EXE/main.exe"
  },
  "author": "Tomoya Sano",
  "license": "ISC",
  "dependencies": {
    "@inquirer/prompts": "^3.3.0",
    "@liveconfig/xterm-webfont": "^2.1.0",
    "@xterm/addon-canvas": "^0.6.0-beta.1",
    "@xterm/addon-webgl": "^0.17.0-beta.1",
    "bcrypt": "^5.1.1",
    "enquirer": "^2.4.1",
    "inquirer": "^9.2.12",
    "neo-blessed": "^0.2.0",
    "neo-blessed-contrib": "^4.9.0",
    "nexe": "^4.0.0-rc.2",
    "node-pty": "^1.0.0",
    "npm-run-all": "^4.1.5",
    "puppeteer": "^19.11.1",
    "puppeteer-cluster": "^0.23.0",
    "puppeteer-core": "^19.11.1",
    "rcedit": "^4.0.1",
    "readline-promises": "^1.3.1",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-node-externals": "^3.0.0",
    "webpack-obfuscator": "^3.5.1",
    "xterm": "^5.3.0",
    "xterm-addon-canvas": "^0.5.0",
    "xterm-addon-fit": "^0.8.0",
    "xterm-addon-web-links": "^0.9.0",
    "xterm-addon-webgl": "^0.16.0"
  },
  "devDependencies": {
    "electron": "^27.1.0",
    "electron-builder": "^24.6.4",
    "electron-packager": "^17.1.2",
    "electron-rebuild": "^3.2.9"
  },
  "type": "module",
  "homepage": "./",
  "build": {
    "productName": "SUS_Login",
    "appId": "com.electron.SUS_Login",
    "directories": {
      "output": "build/"
    },
    "compression": "maximum",
    "files": [
      "src/boot/**/*",
      "package.json"
    ],
    "extraResources": [
      "EXE/*"
    ],
    "win": {
      "icon": "src/assets/icon.ico",
      "target": "nsis"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "deleteAppDataOnUninstall": true
    }
  }
}
